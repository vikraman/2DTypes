    {\arrow["{\ast}"', squiggly, from=1-2, to=2-1]}
    {\arrow[squiggly, from=1-2, to=2-1]}
    \IfBooleanTF{#1}
    {\arrow["{\ast}", squiggly, from=1-2, to=2-3]}
    {\arrow[squiggly, from=1-2, to=2-3]}
